{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "CacheSettings": {
        "UseCachePreload": true,
        "CacheDurationInMinutes": 360
    },
    "UserWebServiceUrl": "http://staging-userwebservice:80",
    "CsvParser": {
        "Delimiter": ",",
        "ColumnId": "User Id"
    },
    "RateLimiting": {
        "Enabled": true,
        "WindowInMinutes": 60,
        "PermitLimit": 10
    }
}